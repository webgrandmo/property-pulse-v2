{
	"trailingComma": "es5",
	"singleQuote": true,
	"semi": true,
	"tabWidth": 4,
	"useTabs": true,
	"printWidth": 100,
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "ignore",
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"embeddedLanguageFormatting": "auto"
}
